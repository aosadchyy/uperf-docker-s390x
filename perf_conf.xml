<?xml version="1.0"?>
<profile name="TCP_RW_JAVA_HTTP">
    <group nthreads="96">
        <transaction duration="30s">
            <!--windsz =8k to match java default socket send and receive buffer size-->
            <flowop type="accept" options="remotehost=uperf-srv protocol=tcp tcp_nodelay wndsz=8k"/>
            <flowop type="write" options="size=2k"/>
            <flowop type="read" options="size=2k"/>
            <flowop type="disconnect" />
        </transaction>
    </group>
</profile>
